{
  "proxyUrl": "/~proxy",
  "maxBodySize": 10000000,
  "proxyToSelfSignedSSL": true,
  "useCORS": true,
  "useSAML": true,
  "redirection": {
    "port": 80
  },
  "proxy": {
    "protocol": "https",
    "port": 8888,
    "httpsCert": "server.cert",
    "httpsKey": "server.key",
    "httpsPassphrase": ""
  },
  "i2b2SessionRequest": {
    "url": "https://localhost/StartSession.php",
    "timeout": 5000,
    "data": {
      "secret": "MY-SHARED-SECRET"
    }
  },
  "gitManager": {
    "active": true,
    "managerUrl": "/~manager",
    "password": "MY-TEAM-PASSWORD",
    "repo": "https://github.com/hms-dbmi/i2b2v2-webclient.git",
    "headName": "master"
  }
}