{
  "proxyUrl": "/~proxy",
  "maxBodySize": 10000000,
  "useCORS": false,
  "SAML": {
    "active": true,
    "type": "I2B2",
    "username": {
      "fromSAML": "nameID",
      "toServer": "X-eduPersonPrincipalName"
    },
    "session": {
      "autogenerate": true,
      "fromSAML": false,
      "toServer": "X-Shib-Session-ID"
    }
  },
  "proxyToSelfSignedSSL": false,
  "redirection": {
    "port": 80
  },
  "proxy": {
    "protocol": "https",
    "port": 443,
    "httpsCert": "server.cert",
    "httpsKey": "server.key",
    "httpsPassphrase": ""
  },
  "logging": {
    "transport": { "target": "pino-pretty" }
  }
}