{
  "proxyUrl": "/~proxy",
  "maxBodySize": 10000000,
  "proxyToSelfSignedSSL": true,
  "useCORS": false,
  "SAML": {
    "active": true,
    "type": "I2B2",
    "username": {
      "fromSAML": "nameID",
      "toServer": "X-eduPersonPrincipalName"
    },
    "session": {
      "autogenerate": true,
      "fromSAML": false,
      "toServer": "X-Shib-Session-ID"
    },
    "secret": {
      "enable": false,
      "secret": "This_is_our_shared_secret!",
      "toServer": "X-i2b2-Security-Hash"
    }
  },
  "redirection": {
    "port": 80
  },
  "proxy": {
    "protocol": "https",
    "port": 443,
    "httpsCert": "server.cert",
    "httpsKey": "server.key",
    "httpsPassphrase": ""
  },
  "i2b2SessionRequest": {
    "url": "https://localhost/StartSession.php",
    "timeout": 5000,
    "data": {
      "secret": "MY-SHARED-SECRET"
    }
  },
  "gitManager": {
    "active": true,
    "managerUrl": "/~manager",
    "password": "MY-TEAM-PASSWORD",
    "repo": "https://github.com/hms-dbmi/i2b2v2-webclient.git",
    "headName": "master"
  },
  "logging": {
    "transport": { "target": "pino-pretty" }
  }
}